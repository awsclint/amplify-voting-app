{
	"auth": {
		"VotingAppAuth": {
			"service": "Cognito",
			"providerPlugin": "awscloudformation"
		}
	},
	"storage": {
		"VotingAppVideos": {
			"service": "DynamoDB",
			"providerPlugin": "awscloudformation"
		},
		"VotingAppVotes": {
			"service": "DynamoDB",
			"providerPlugin": "awscloudformation"
		}
	},
	"function": {
		"VotingAppGetVideos": {
			"service": "Lambda",
			"providerPlugin": "awscloudformation",
			"build": true,
			"dependsOn": [
				{
					"category": "storage",
					"resourceName": "VotingAppVideos",
					"attributes": [
						"Name",
						"Arn"
					]
				}
			]
		}
	},
	"api": {
		"VotingAppAPI": {
			"service": "API Gateway",
			"providerPlugin": "awscloudformation",
			"dependsOn": [
				{
					"category": "function",
					"resourceName": "VotingAppGetVideos",
					"attributes": [
						"Name",
						"Arn"
					]
				}
			]
		}
	}
}